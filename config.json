{
  "myservice": {
    "$filter": "env",
    "production": {
      "url": {
        "$filter": "country",
        "ES": "http://myservice-production.es",
        "UK": "http://myservice-production.co.uk"
      },
    },
    "development": {
      "url": "http://myservice-production.com" 
    }
  },
    "key1": "value",
    "key2": {
        "$filter": "env",
        "production": {
            "limit": 200
        },
        "$default": {
            "limit": 12
        }
    },
  "color": {
    "$filter": "random",
    "$range": [
      { "limit": 10, "value": "red" },
      { "limit": 20, "value": "green" }
    ],
    "$default": "blue"
  }
}
